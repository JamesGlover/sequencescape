
<% unless @users.nil? || @users.empty? %>
  <table class='sortable table table-striped'>
    <thead>
      <tr>
        <th>Name</th>
        <th>Login</th>
        <th>Joined</th>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
      </tr>
    </thead>
    <tbody>
        <%= render partial: "admin/users/user", collection: @users %>
    </tbody>
  </table>
<% else %>
  <h3>No users were found</h3>
<% end %>
